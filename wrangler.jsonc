// Cloudflare Pages configuration for Astro project
{
  "name": "devin",
  "compatibility_date": "2025-05-21",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "main": "dist/_worker.js",
  
  // Build settings
  "build": {
    "command": "npm run build"
  },

  // Configuration specific to Astro
  "site": {
    "bucket": "./dist"
  },
  
  // Environment variables (customize as needed)
  "observability": {
    "logs": {
      "enabled": true
    }
  }
}
